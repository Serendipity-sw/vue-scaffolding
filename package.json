{
  "name": "vue-scaffolding",
  "version": "1.0.0",
  "description": "scaffolding",
  "scripts": {
    "zip": "gulp buildTime && gulp zip",
    "start": "webpack serve --progress --mode development -c ./config/webpack/webpack.dev.js",
    "analyze": "webpack --progress --mode production -c ./config/webpack/webpack.analyze.js",
    "build-develop-fix": "webpack --progress --mode production -c ./config/webpack/webpack.dev.js",
    "build-release": "webpack --progress --mode production -c ./config/webpack/webpack.release.js",
    "build": "webpack --progress --mode production -c ./config/webpack/webpack.product.js",
    "build-develop-fix:zip": "yarn run build-develop-fix && yarn run zip",
    "build-release:zip": "yarn run build-release && yarn run zip",
    "build:zip": "yarn run build && yarn run zip",
    "format": "prettier --write ./src",
    "lint": "eslint src/**/*.{js,vue,ts,jsx,tsx} --fix",
    "upload-build-develop-fix": "yarn run build-develop-fix && gulp buildTime && gulp uploadDevelop",
    "upload-build-release": "yarn run build-release && gulp buildTime && gulp uploadRelease"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swgloomy/react-scaffolding.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swgloomy/vue-scaffolding/issues"
  },
  "homepage": "https://github.com/swgloomy/vue-scaffolding#readme",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^9.1.2",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
